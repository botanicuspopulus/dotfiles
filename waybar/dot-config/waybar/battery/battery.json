{
    "battery": {
        "interval": 60,
        "states": {
            "full": 100,
            "not-full": 99,
            "warning": 20,
            "critical": 10
        },
        "format": "{icon} {capacity}% {time}",
        "format-icons" : ["󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹"],
        "format-time": "({H}:{m})",
        "format-charging": "󰂄 {capacity}% {time}",
        "format-full": " {icon}",
        "tooltip": false
    }
}
