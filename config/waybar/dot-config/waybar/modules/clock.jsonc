{
  "clock": {
    "interval": 1,
    "format": "󱑎  {:%H:%M:%S}",
    "format-alt": "󰃭 {:%a %b %d %Y}",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "week-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
    },
    "actions": {
      "on-click-right": "mode",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
  },
}
