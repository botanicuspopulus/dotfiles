
# Start launcher
bindsym $mod+d exec --no-startup-id ~/.config/rofi/launcher/launcher.sh
bindsym --to-code $mod+b exec firefox

# Power Menu
bindsym $mod+p exec --no-startup-id ~/.config/rofi/powermenu/powermenu.sh

# Launch wifi choice
bindsym --to-code $mod+n  exec ~/.config/rofi/wifi/rofi-wifi-menu.sh
