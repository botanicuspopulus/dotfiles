
# Start launcher
bindsym $mod+d exec $menu
bindsym --to-code $mod+b exec firefox

# Power Menu
bindsym $mod+p exec ~/.config/bin/wofi-power
bindsym $mod+Tab exec ~/.config/bin/wofi-windows

# Launch wifi choice
bindsym --to-code $mod+n  exec $term -e nmtui-connect

# Launch categorized menu
bindsym --to-code $mod+a exec --no-startup-id $menu
bindsym --to-code $mod+x exec wofi
